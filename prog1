pipeline { 
    agent any
    stages {
        stage('Clone Git') {
            steps {
                git 'https://github.com/abhipsapanda2002/jenkins_demo.git'
            }
        }
        stage('Build Code') {
            steps {
                sh "chmod u+x prog1"
                sh "./prog1"
            }
        }
     stage('Test Code') {
            steps {
                sh "chmod u+x Test.py"
                sh "./Test.py"
            }
        }
    } 
}
